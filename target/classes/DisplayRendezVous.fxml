<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>



<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="view.DisplayRendezVousController"
            prefHeight="400.0" prefWidth="600.0">

    <ScrollPane prefHeight="400.0" prefWidth="600.0">
        <content>
            <AnchorPane>
                <VBox alignment="CENTER" spacing="10">
                    <Label style="-fx-font-size: 20px; -fx-font-weight: bold;" text="RendezVous List" />
                    <VBox fx:id="rendezVousList" alignment="CENTER" spacing="5">

                    </VBox>
                </VBox>
            </AnchorPane>
        </content>
    </ScrollPane>
    <VBox layoutX="348.0" layoutY="14.0" prefHeight="42.0" prefWidth="42.0">
        <children>
            <Button alignment="CENTER_RIGHT" onAction="#navigateToAddInterface" text="Add" />
        </children>
    </VBox>
    <VBox layoutX="400.0" layoutY="14.0" prefHeight="56.0" prefWidth="87.0">
        <children>
            <Button alignment="CENTER_RIGHT" onAction="#navigateToPlaceInterface" text="Place" />
        </children>
    </VBox>


</AnchorPane>
